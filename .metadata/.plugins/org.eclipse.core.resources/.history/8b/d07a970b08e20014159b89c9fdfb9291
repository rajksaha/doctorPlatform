  $scope.getProducts = function(term) {
    $scope.products = [];
    
    var  dataString='data='+  $scope.docName +'&query='+5;
    
    ItemService.search.query({}, dataString).$promise.then(function(result) {
    	$scope.products = result;
    	return $scope.products;
        });
        return $scope.products;
      };

